#!/bin/sh

set -e

if [ "x$1" = xremove ]; then
	update-alternatives \
		--remove default.plymouth /usr/share/plymouth/themes/budgie-remix-logo/budgie-remix-logo.plymouth
	update-alternatives \
		--remove default.plymouth /usr/share/plymouth/themes/budgie-remix-logo/budgie-remix-logo-scale-2.plymouth
fi


